{
  "name": "@verifiedinc-public/shared-ui-elements",
  "version": "7.6.1",
  "description": "A set of UI components, utilities that is shareable with the core apps.",
  "private": false,
  "keywords": [],
  "author": "VerifiedInc",
  "engines": {
    "node": ">=20.10.0",
    "npm": ">=10.x.x"
  },
  "main": "/dist/index.mjs",
  "types": "/dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.mjs"
    },
    "./components/*": {
      "types": "./dist/components/*/index.d.ts",
      "import": "./dist/components/*/index.mjs"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.mjs"
    },
    "./styles": {
      "types": "./dist/styles/index.d.ts",
      "import": "./dist/styles/index.mjs"
    },
    "./validations": {
      "types": "./dist/validations/index.d.ts",
      "import": "./dist/validations/index.mjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.mjs"
    },
    "./utils/*": {
      "types": "./dist/utils/*/index.d.ts",
      "import": "./dist/utils/*/index.mjs"
    }
  },
  "scripts": {
    "test": "npm run vitest",
    "vitest": "vitest --run",
    "lint": "npm run eslint && npm run prettier:check && npm run typecheck",
    "eslint": "eslint",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "typecheck": "tsc -b",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prebuild": "rm -rf dist",
    "build": "npm run compile",
    "compile": "tsc && vite build && find dist -name '*.mjs' -type f -exec esbuild {} --minify --outfile={} --allow-overwrite \\;",
    "prepublishOnly": "npm run build",
    "publish:prerelease": "npm version prerelease --preid=beta && npm publish --tag beta"
  },
  "dependencies": {
    "react-datepicker": "^7.6.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@chromatic-com/storybook": "^2.0.2",
    "@storybook/addon-essentials": "^8.3.1",
    "@storybook/addon-interactions": "^8.3.1",
    "@storybook/addon-links": "^8.3.1",
    "@storybook/addon-onboarding": "^8.3.1",
    "@storybook/addon-themes": "^8.3.1",
    "@storybook/addon-viewport": "^8.3.1",
    "@storybook/blocks": "^8.3.1",
    "@storybook/react": "^8.3.1",
    "@storybook/react-vite": "^8.3.1",
    "@storybook/test": "^8.3.1",
    "@testing-library/dom": "^10.3.0",
    "@testing-library/react": "^16.0.0",
    "@types/lodash": "^4.17.16",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/tinycolor2": "^1.4.6",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@vitejs/plugin-react": "^4.3.1",
    "ajv": "^8.17.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vitest": "^0.0.53",
    "jsdom": "^24.1.0",
    "prettier": "^3.3.3",
    "storybook": "^8.3.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^3.12.4",
    "typescript": "^5.2.2",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "@date-fns/tz": "^1.2.0",
    "@dotlottie/react-player": "^1.6.19",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/lato": "^5.0.21",
    "@fontsource/material-icons": "^5.0.18",
    "@hookform/resolvers": "^3",
    "@mona-health/react-input-mask": "^3.0.3",
    "@mui/icons-material": ">=5 <6",
    "@mui/material": ">=5 <6",
    "@tanstack/react-query": ">=5",
    "decimal.js": ">=10.4.3",
    "framer-motion": ">=11.11.1",
    "immer": ">=10",
    "html-to-image": ">=1.11.13 <2",
    "libphonenumber-js": ">=1.11.11",
    "notistack": ">=3.0.1",
    "qr-code-styling": ">=1.9.2 <2",
    "qrcode": ">=1.5.4",
    "react": ">=18",
    "react-dnd": ">=16.0.1",
    "react-dnd-html5-backend": ">=16.0.1",
    "react-dom": ">=18",
    "react-hook-form": ">=7.52.1 <=7.53.2",
    "react-imask": ">=7.6.1",
    "react-to-pdf": ">=2.0.0 <3",
    "recharts": ">=2.15.0 <3",
    "tinycolor2": ">=1.6.0 <2",
    "zod": ">=3.23.8 <4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.9.5"
  }
}
